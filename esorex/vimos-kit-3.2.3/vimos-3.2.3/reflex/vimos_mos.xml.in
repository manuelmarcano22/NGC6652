            <entity name="GetParamsFromGrismTable" class="ptolemy.actor.lib.python.PythonScript">
                <property name="jythonClassName" class="ptolemy.kernel.util.StringAttribute" value="Main">
                </property>
                <property name="script" class="ptolemy.kernel.util.StringAttribute" value="# This is a simple actor that copies the input to the output.&#10;# You can remove the ports, add new ports, and modify the script.&#10;&#10;from ptolemy.data import StringToken&#10;from org.eso.util import JSONTools&#10;from nom.tam.fits import Fits&#10;from nom.tam.fits import FitsFactory&#10;&#10;class Main :&#10;  &quot;Get the parameters from the config table&quot;&#10;  def fire(self) :&#10;    FitsFactory.setUseHierarch(True)&#10;    token = self.input.get(0)&#10;    sof = JSONTools.fromJSON(token.stringValue())&#10;    for file in sof.getFiles() :&#10;      f = Fits(file.getName())&#10;      key = f.getHDU(0).getHeader().findCard('HIERARCH.ESO.PRO.CATG')&#10;      if key is not None:&#10;        cat = key.getValue()&#10;        try :&#10;          if cat[0:] == 'CONFIG_TABLE':&#10;            table = f.getHDU(1)&#10;            dispersion = table.getElement(0,table.findColumn('dispersion'))[0]&#10;            out = &quot;vmmoscalib:dispersion=&quot;+str(dispersion)+&quot;\n&quot;&#10;            peakdetection = table.getElement(0,table.findColumn('peakdetection'))[0]&#10;            out = out + &quot;vmmoscalib:peakdetection=&quot;+str(peakdetection)+&quot;\n&quot;&#10;            wdegree = table.getElement(0,table.findColumn('wdegree'))[0]&#10;            out = out + &quot;vmmoscalib:wdegree=&quot;+str(wdegree)+&quot;\n&quot;&#10;            cdegree = table.getElement(0,table.findColumn('cdegree'))[0]&#10;            out = out + &quot;vmmoscalib:cdegree=&quot;+str(cdegree)+&quot;\n&quot;&#10;            startwavelength = table.getElement(0,table.findColumn('startwavelength'))[0]&#10;            out = out + &quot;vmmoscalib:startwavelength=&quot;+str(startwavelength)+&quot;\n&quot;&#10;            endwavelength = table.getElement(0,table.findColumn('endwavelength'))[0]&#10;            out = out + &quot;vmmoscalib:endwavelength=&quot;+str(endwavelength)+&quot;\n&quot;&#10;            reference = table.getElement(0,table.findColumn('reference'))[0]&#10;            out = out + &quot;vmmoscalib:reference=&quot;+str(reference)+&quot;\n&quot;&#10;            self.output.broadcast(StringToken(out))&#10;            return&#10;        except :&#10;          raise&#10;          message='Problem reading grism config table (file %s)'%(sof.getFiles()[0].getName())&#10;          raise Exception(message)&#10;    self.output.broadcast(StringToken(''))&#10;    return&#10;&#10;">
                </property>
