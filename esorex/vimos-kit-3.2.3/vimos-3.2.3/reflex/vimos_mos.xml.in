        <entity name="SetInitialLoopParam" class="ptolemy.actor.lib.python.PythonScript">
            <property name="jythonClassName" class="ptolemy.kernel.util.StringAttribute" value="Main">
            </property>
            <property name="script" class="ptolemy.kernel.util.StringAttribute" value="# This actor will set INIT_ parameters using the provided values.&#10;# This actor will typically be inserted after an interactive PythonActor&#10;# and will be used to set the default parameters for the&#10;# subsequent executions of the recipe.&#10;&#10;&#10;import ptolemy.actor.util.ExplicitChangeContext&#10;import ptolemy.data.expr.ModelScope&#10;import ptolemy.data.expr.Variable&#10;import ptolemy.data.Token&#10;&#10;class Main:&#10;  &quot;Set Default Values&quot;&#10;&#10;  def fire(self) :&#10;&#10;    input = self.sop_in.get(0)&#10;    string = input.stringValue()&#10;    obj = JSONTools.fromJSON(string)&#10;    params = obj&#10;&#10;    #Get the container with all the objects (this is where we will look for the variables)&#10;    container = self.actor.getContainer()&#10;&#10;    #Loop through the parameters&#10;    for param in params:&#10;      init_name = 'init_'+param.getDisplayName()&#10;      new_value = param.getValue()&#10;      #Get the ptolemy attribute with that name&#10;      attribute =  ptolemy.data.expr.ModelScope.getScopedAttribute(None, container, init_name)&#10;      if(attribute == None) :&#10;        #Search also for the upper case name&#10;        init_name = init_name.upper()&#10;        attribute =  ptolemy.data.expr.ModelScope.getScopedAttribute(None, container, init_name)&#10;        if(attribute == None) :&#10;          continue&#10;      oldToken = attribute.getToken()&#10;      attribute.setExpression(str(new_value))&#10;      # NOTE: If we don't call validate(), then the change will not propagate to dependents.&#10;      attribute.validate()&#10;&#10;    return&#10;&#10;">
            </property>
